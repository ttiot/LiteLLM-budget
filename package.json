{
  "name": "LiteLLM-budget",
  "displayName": "LiteLLM Budget",
  "description": "Display the spend and max budget from LiteLLM",
  "version": "0.0.5",
  "publisher": "jmichaux",
  "repository": "https://github.com/ttiot/LiteLLM-budget",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "LiteLLM",
      "properties": {
        "LiteLLM.refreshInterval": {
          "type": "number",
          "default": 60,
          "description": "Intervalle de rafra√Æchissement en secondes"
        },
        "LiteLLM.apiUrl": {
          "type": "string",
          "default": "https://",
          "description": "URL de base de l'API LiteLLM"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "node-fetch": "^2.6.12"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
